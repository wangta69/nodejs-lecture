doctype html
html(lang='en')
  head
    title List Page
    link(rel='stylesheet', type='text/css', href='/assets/css/default.css')
  body
    div.top-gnb
      a(href='/') Home |
      a(href='/users') users
    div.body
    a(href='/write') [등록하기]
    ul.list
      li
        span email
        span name
        span password
        span
      each itme in rows
        li
          span= itme.email
          span= itme.name
          span= itme.password
          span
            a(href='/view/'+itme.id) [view]
            a(href='/update/'+itme.id) [modify]
            a(href='/delete/'+itme.id) [delete]
